@model Cart
@if (Model != null)
{
    @foreach (var item in Model.GetCartItems)
    {
        <tr>
            <td>@item.Title </td>
            <td>@item.Price </td>
            <td><input type="number" value="@item.Quantity" min="0" /> </td>
            <td>@item.Discount</td>
            <td>@item.SubTotal</td>
        </tr>
    }
}

else
{
<h3>@ViewBag.Message </h3>

}
